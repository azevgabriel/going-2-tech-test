  "scripts": {
    "dev": "nest start --watch node --env-file=.env",
    "test": "node --env-file=.env -r ts-node/register node_modules/.bin/jest --config ./jest.json",
    "test:watch": "node --env-file=.env -r ts-node/register node_modules/.bin/jest jest --watch",
    "migration:run": "node --env-file=.env -r ts-node/register node_modules/typeorm/cli.js migration:run -d ./src/db/typeorm/index.ts",
    "migration:revert": "node --env-file=.env -r ts-node/register node_modules/typeorm/cli.js migration:revert -d ./src/db/typeorm/index.ts",
    "seed": "node --env-file=.env -r ts-node/register ./src/db/typeorm/seed"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^5.1.1",
    "pg": "^8.13.3",
    "postgres": "^3.4.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
